#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> nums(n), ans(n);
    for (int i = 0; i < n; i++)
        cin >> nums[i];

    int p = 0, q = 1;
    for (int i = 0; i < n; i++) {
        if (nums[i] >= 0) {
            ans[p] = nums[i];
            p += 2;
        } else {
            ans[q] = nums[i];
            q += 2;
        }
    }

    for (int i = 0; i < n; i++)
        cout << ans[i] << " ";

    return 0;
}
