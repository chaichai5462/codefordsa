#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> a(n);
    for (int i = 0; i < n; i++) {
        cin >> a[i];
        a[i] = a[i] * a[i];
    }

    sort(a.begin(), a.end());

    for (int i = n - 1; i >= 2; i--) {
        int l = 0, r = i - 1;
        while (l < r) {
            if (a[l] + a[r] == a[i]) {
                cout << "Yes";
                return 0;
            } else if (a[l] + a[r] < a[i]) {
                l++;
            } else {
                r--;
            }
        }
    }

    cout << "No";
    return 0;
}
