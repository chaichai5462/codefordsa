#include <iostream>
using namespace std;

class CircularQueue {
    int q[5];
    int front, rear;

public:
    CircularQueue() {
        front = -1;
        rear = -1;
    }

    // Insert element
    void enqueue(int x) {
        if ((front == 0 && rear == 4) || (front == rear + 1)) {
            cout << "Queue Overflow\n";
            return;
        }

        if (front == -1) {   // first insertion
            front = rear = 0;
        } else if (rear == 4) {
            rear = 0;
        } else {
            rear++;
        }

        q[rear] = x;
    }

    // Delete element
    void dequeue() {
        if (front == -1) {
            cout << "Queue Underflow\n";
            return;
        }

        cout << "Deleted: " << q[front] << endl;

        if (front == rear) {   // only one element
            front = rear = -1;
        } else if (front == 4) {
            front = 0;
        } else {
            front++;
        }
    }

    // Display queue
    void display() {
        if (front == -1) {
            cout << "Queue is empty\n";
            return;
        }

        int i = front;
        while (true) {
            cout << q[i] << " ";
            if (i == rear)
                break;
            i = (i + 1) % 5;
        }
        cout << endl;
    }
};

int main() {
    CircularQueue cq;
    cq.enqueue(10);
    cq.enqueue(20);
    cq.enqueue(30);
    cq.display();

    cq.dequeue();
    cq.display();

    cq.enqueue(40);
    cq.enqueue(50);
    cq.display();

    return 0;
}
