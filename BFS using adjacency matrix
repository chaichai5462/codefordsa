#include <iostream>
#include <queue>
using namespace std;

int main() {
    int n;
    int adj[10][10];
    int visited[10] = {0};

    cout << "Enter number of vertices: ";
    cin >> n;

    cout << "Enter adjacency matrix:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> adj[i][j];
        }
    }

    queue<int> q;
    int start = 0;          // starting vertex
    q.push(start);
    visited[start] = 1;

    cout << "BFS Traversal: ";

    while (!q.empty()) {
        int v = q.front();
        q.pop();
        cout << v << " ";

        for (int i = 0; i < n; i++) {
            if (adj[v][i] == 1 && visited[i] == 0) {
                q.push(i);
                visited[i] = 1;
            }
        }
    }

    return 0;
}

